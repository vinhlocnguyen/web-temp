{
  "name": "Drops-Web",
  "version": "0.2.0",
  "description": "Web Application looks for mobility data and services nearby a specific building.",
  "scripts": {
    "start": "NODE_ENV=production yarn run build && yarn run start:server",
    "start:dev": "NODE_ENV=development yarn run build:server:dev && NODE_ENV=development yarn run start:server:dev",
    "build": "yarn run build:server && yarn run build:client",
    "build:server:dev": "webpack --config webpack.server.config.js",
    "build:server": "NODE_ENV=production webpack -p --config webpack.server.config.js",
    "build:client": "NODE_ENV=production webpack -p --config webpack.config.js",
    "start:server": "node build/server.bundle.js",
    "start:server:dev": "API_KEY=12345678 node build/server.bundle.js",
    "lint": "node node_modules/eslint/bin/eslint.js ./src/",
    "flow": "flow check",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha -r mock-local-storage --recursive ./tests/helpers/browser.js ./tests/* --compilers js:babel-register",
    "test:auto": "nodemon --exec 'NODE_ENV=test mocha -r mock-local-storage --recursive ./tests/helpers/browser.js ./tests/* --compilers js:babel-register || true'",
    "bundle-size": "webpack --json | webpack-bundle-size-analyzer",
    "plato": "./node_modules/.bin/plato -r -d report src && open ./report/index.html",
    "prune": "./yarn-prune.js"
  },
  "nyc": {
    "extension": [
      ".jsx",
      ".es6"
    ]
  },
